#include <iostream>
#include <stack>
using namespace std;

int main() 
{
    stack<string> s;
    
    s.push("eahh4");
    s.push("yeah3");
    s.push("yooh2");
    s.push("hooo1");
    cout<<"top ele is:  "<<s.top()<<endl;
    
    s.pop();
    cout<<"top ele is:  "<<s.top()<<endl;
    
    cout<<"size of stack:  "<<s.size()<<endl;
    cout<<"empty or not:  "<<s.empty()<<endl;
    
    while (!s.empty()) 
    {
        cout << s.top() <<"   ";
        s.pop();
    }
    
    
    
    return 0;
}
