#include <iostream>
#include <array>
using namespace std;

int main() {
    
    array<int,6> a={34,56,11,47,90,43};
    int size=a.size();
    for (int i=0;i<size;i++)
    {
        cout<<a[i]<<endl;
    }
    
    cout<<endl<<"first ele  "<<a.front()<<endl;
    cout<<"last ele  "<<a.back()<<endl;
    cout<<"ele at index 3 or 4th position   "<<a.at(3)<<endl;
    
    return 0;
}
